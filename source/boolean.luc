module boolean (
    input a[16],  
    input b[16],
    input alufn[6],
    output out[16]
  ) {
  
  var i;
  var mulselector;

  always {
    for (i=0; i<16; i++){
      mulselector = b[i]*2 + a[i];
    case (mulselector){
      b00:
      out[i] = alufn[0];
      b01:
      out[i] = alufn[1];
      b10:
      out[i] = alufn[2]; 
      b11:
      out[i]= alufn[3]; 
      default:
      out[i]=alufn[0];
      }
    }
  }
}