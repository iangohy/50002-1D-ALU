module compare (
    input z,
    input v,
    input n,
    input alufn[3],
    output c
  ) {

  always {
    c = 0;
    case(alufn[2:1]){
      b00:
        // None
        c = 0;
      b10:
        // Less Than
        c = n^v;
      b01:
        // Equal
        c = z;
      b11:
        // Less than/Equal to
        c = (n^v) | z;
          
  }
}
